	"repository": "github:drovp/optimize",
	"main": "dist/index.js",
	"keywords": [
		"drovpplugin",
		"optimize",
		"compress",
		"image",
		"jpg",
		"png",
		"gif",
		"webp",
		"svg",
		"mozjpeg",
		"libwebp",
		"pngquant",
		"optipng",
		"gifsicle",
		"gif2webp",
		"svgo"
	],
	"author": "tomasklaen",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
